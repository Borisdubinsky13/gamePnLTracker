<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="gamePnLTracker.jar"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/appname">
		<activity android:name=".DataEntry"
	    		android:label="@string/appname">
			<intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
	    </activity>
		<activity android:name=".SetupWin"
					android:label="@string/setupName">
			<intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
		</activity>    
        <activity android:name=".loginHandler"
                  android:label="@string/appname">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
	    <activity android:name=".AboutHandler"
	    		android:label="@string/appname">
			<intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
	    </activity>
	    <activity android:name=".AfterLogin"
			android:label="@string/appname">
			<intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <action android:name="android.intent.action.PICK" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
		</activity>
		<activity android:name=".ListRes"
				android:label="@string/listRes">
				<intent-filter>
					<action android:name="android.intent.action.VIEW" />
					<action android:name="android.intent.action.PICK" />
					<category android:name="android.intent.category.DEFAULT" />
				</intent-filter>
		</activity>
		<provider android:name="GameContentProvider"
			android:authorities="gamePnLTracker.provider.userContentProvider"/>
	</application>
</manifest> 